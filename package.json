{
  "name": "node-jsx-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:client": "NODE_ENV=production webpack -p --progress",
    "build:server": "babel index.js --out-file server.js",
    "clean": "rimraf dist/* && ([ -d dist ] || mkdir dist)",
    "dev:start": "nodemon -e js,jsx --exec babel-node index",
    "dev:wds": "webpack-dev-server --progress",
    "serve": "NODE_ENV=production node server.js",
    "start": "npm-run-all --parallel dev:*"
  },
  "dependencies": {
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "express-react-views": "^0.10.2",
    "helmet": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "nodemon": "^1.11.0",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.1.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1"
  }
}
